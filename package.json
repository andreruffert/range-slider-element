{
  "name": "range-slider-element",
  "version": "2.0.0-rc.6",
  "description": "A customizable range slider element",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/range-slider-element.umd.cjs",
  "module": "./dist/range-slider-element.js",
  "exports": {
    ".": {
      "import": "./dist/range-slider-element.js",
      "require": "./dist/range-slider-element.umd.cjs"
    },
    "./style.css": "./dist/range-slider-element.css"
  },
  "scripts": {
    "dev": "vite",
    "dev:docs": "vite --config vite.docs.config.js",
    "format": "biome check --write",
    "lint": "biome ci",
    "test": "vitest",
    "prebuild": "rm -rf dist",
    "build": "vite build",
    "build:docs": "vite build --config vite.docs.config.js",
    "prepublishOnly": "npm run build",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreruffert/range-slider-element.git"
  },
  "author": "Andr√© Ruffert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andreruffert/range-slider-element/issues"
  },
  "homepage": "https://github.com/andreruffert/range-slider-element#readme",
  "keywords": [
    "range slider",
    "custom elements",
    "web components",
    "ui"
  ],
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@vitest/browser": "3.0.5",
    "conventional-changelog-cli": "^2.1.1",
    "playwright": "1.50.1",
    "vite": "6.0.11",
    "vitest": "3.0.5"
  },
  "volta": {
    "node": "22.13.1",
    "npm": "11.1.0"
  }
}
